{
  "expo": {
    "name": "minimal-reproduction-rn-bottom-tabs",
    "slug": "minimal-reproduction-rn-bottom-tabs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "backgroundColor": "#38EF14",
    "plugins": [
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "react-native-edge-to-edge"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#EE4B2B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beauhouse.minimalreproductionrnbottomtabs"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "db3dbf82-63c4-4bf1-8a5f-90ae8d98f0d7"
      }
    }
  }
}
